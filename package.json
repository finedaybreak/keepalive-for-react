{
  "name": "keepalive-for-react",
  "version": "2.0.0",
  "description": "A react <KeepAlive/> component like <keep-alive/> in vue, it can cache component state when component unmount, and restore state when component mount again.",
  "homepage": "https://github.com/irychen/keepalive-for-react",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irychen/keepalive-for-react.git"
  },
  "files": [
    "dist/types/**/*.d.ts",
    "dist/esm/index.mjs"
  ],
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write"
    ]
  },
  "type": "module",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:main": "rollup --config rollup.config.js",
    "build:types": "tsc -b ./tsconfig.types.json",
    "build": "npm run build:main && npm run build:types"
  },
  "keywords": [
    "KeepAlive",
    "keepalive",
    "Keep Alive",
    "keep-alive",
    "React Keepalive",
    "react keepalive",
    "React Keep Alive",
    "cache component",
    "mutiple tabs",
    "keepalive for react",
    "keep-alive for react",
    "keepalive-for-react"
  ],
  "author": "wongyichen",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/runtime": "^7.23.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "prettier": "^3.0.3",
    "rollup": "^4.0.2",
    "rollup-plugin-ts": "^3.4.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
