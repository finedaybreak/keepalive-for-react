    "description": "A react <KeepAlive/> component like <keep-alive/> in vue",
    "homepage": "https://github.com/finedaybreak/keepalive-for-react",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/finedaybreak/keepalive-for-react.git"
    },
    "exports": {
        ".": {
